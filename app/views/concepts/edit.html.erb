<div class='container'>
	<div class='content'>
		<div class='row'>
			<div class='concept-header col-md-12'>
				<i class="fa fa-pencil"></i>
				<%= link_to(current_user.profile_name, profiles_show_path, class: 'header-profile')  + ' / ' + @concept.title %>
				<span class='pull-right'>
					<%= link_to 'Show', @concept %> |
					<%= link_to 'Back', concepts_path %>
				</span>
		</div> <!-- end row -->
		<div class="row">
			<div class="col-md-12">
					<%= render 'form' %>
			</div>
		</div> <!-- end row -->
	</div>
</div>